.imgcut .imgcut__main {
    display: -webkit-flex;
    display: flex;
    -webkit-align-items: center;
    align-items: center;
    -webkit-justify-content: center;
    justify-content: center;
    background: #000;
}

.imgcut__main wx-view {
    border: 1px solid #ffe000;
}

.btn-cancel, .btn-complete {
    width: 52px;
    height: 38px;
    line-height: 38px;
    padding: 0px;
    text-align: center;
    border-radius: 0;
    color: #fff;
    font-size: 15px;
    background-color: transparent;
}

.btn-cancel::after, .btn-complete::after {
    border-radius: 0;
}

.btn-complete {
    float: right;
}

.btn-cancel {
    float: left;
}

.imgcutBtn {
    background-color: #151515;
    height: 38px;
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
}